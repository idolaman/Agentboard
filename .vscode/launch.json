{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Thinking Logger UI",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/thinking-logger-ui"
      ],
      "outFiles": [
        "${workspaceFolder}/thinking-logger-ui/dist/**/*.js"
      ],
      "preLaunchTask": "build-thinking-logger-ui"
    }
  ]
}
